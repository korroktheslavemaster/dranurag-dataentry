    var evtLabel = $(this).text().split('(')[0] + '_strength_' + $(this).text().split('(')[1].split(')')[0];
    pushToDataLayer({eventCategory: 'Drugs', eventAction: 'Salt Information', eventLabel: evtLabel, event: 'newGAEvent'});
  });
});
document.addEventListener('readystatechange', function (e) {
  if(e.target.readyState === 'complete' && 'en' === 'en') {
    dfpAddLoaded();
  }
});
var expCallback = function() {
  grecaptcha.reset();
};

var productValue = parseFloat(dataLayer.mrp);
var rangeStart = 0;
var rangeString;
var manufacturerId = null;
var productType = null;
var productsId = null;
var user_city = '';
// selecting range based on product mrp
while (rangeStart <= 1900) {
  if (productValue >= rangeStart && productValue < (rangeStart + 100)) {
    rangeString = rangeStart.toString() + '-' + (rangeStart + 100).toString();
    break;
  }
  if (rangeStart >= 1900) {
    rangeString = '2000 above';
    break;
  } else {
    rangeStart = rangeStart + 100;
  }
}
for (var i=0; i<dataLayer.length; i++) {
  if (dataLayer[i].mfId) {
    manufacturerId = dataLayer[i].mfId;
  }
  if (dataLayer[i].skuType) {
    productType = dataLayer[i].skuType;
  }
  if (dataLayer[i].id) {
    productsId = dataLayer[i].id;
  }
  if (dataLayer[i].city) {
    user_city = dataLayer[i].city;
  }
}
googletag.cmd.push(function() {
  googletag.defineSlot('/67416580/WEB_1MG_PRODUCT_DRUG_TOP_RIGHT_ATF_0_250x250', [250, 250], 'div-gpt-ad-1471501284459-0')
    .addService(googletag.pubads())
    .setTargeting("meta_therapy_area", JSON.parse(localStorage.getItem("therapeuticClasses")))
    .setTargeting("meta_manufacturer_id", JSON.parse(localStorage.getItem("manufacturerIds")))
    .setTargeting("meta_sku_type", JSON.parse(localStorage.getItem("skuTypes")))
    .setTargeting("meta_cart_value", JSON.parse(localStorage.getItem("cartValues")))
    // add targeting for 'product' product related local values
    .setTargeting("prod_manufacturer_id", manufacturerId)
    .setTargeting("prod_sku_type", productType)
    .setTargeting("prod_value", rangeString)
    .setTargeting("prod_sku_id", productsId)
    // add targeting for city value
    .setTargeting("user_city", user_city);
  googletag.pubads().enableSingleRequest();
  googletag.enableServices();
});</script><div class="push"></div></body></html>